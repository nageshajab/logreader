<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../partials/head'); %>
</head>

<body class="container-fluid p-0 pl-1 pr-1">

  <header>
    <%- include('../partials/header'); %>
  </header>
  <!--   <% if (messages.error) { %>
  <p style="color:red"><%- messages.error %></p>
<% } %> -->

  <% if (messages.success) { %>
  <p class="alert alert-success mt-4"><%- messages.success %></p>
  <% } %>
  <br>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">#</th>
        <th scope="col">Datetime</th>
        <th scope="col">Host</th>
        <th scope="col">ServiceName</th>
        <th scope="col">Message</th>
      </tr>
    </thead>
    <tbody>
      <% if(data.length){
 
  for(var i = 0; i< data.length; i++) {%>
      <tr>
        <th scope="row"><%= (i+1) %></th>
        <td><%=  data[i].datetime.toString().substr(0,20) %></td>
        <td><%= data[i].host %></td>
        <td><%= data[i].serviceName.substr(data[i].serviceName.length-20,20) %></td>
        <td><%= data[i].message %></td>
      </tr>
      <% }
           
   }else{ %>
      <tr>
        <td colspan="3">No user</td>
      </tr>
      <% } %>

    </tbody>
  </table>
  <footer>
    <%- include('../partials/footer'); %>
  </footer>

</body>

</html>